<script>
$(document).ready(function(){
	var open = false;
	
	function toggleNav(){
		$("body").toggleClass("navOpen");
		$("nav").toggleClass("open");
		$(".wrapper").toggleClass("open");
		$("nav.left-nav ul").toggle();
		$('.openNav').toggleClass("open");
		open = !open;
		if(open){
			console.log('opened');
			setTimeout(function(){
							$(document).on('click',toggleNav);
			},500)
		}else{
			console.log('closed');
			$(document).off('click');
		}
	}
	$(".openNav").click(toggleNav);

var step = 100;
var scrolling = false;

// Wire up events for the 'scrollUp' link:
$(".up").bind("click", function(event) {
    event.preventDefault();
    // Animates the scrollTop property by the specified
    // step.
    $(".bid-list").animate({
        scrollTop: "-=" + step + "px"
    });
}).bind("mouseover", function(event) {
    scrolling = true;
    scrollContent("up");
}).bind("mouseout", function(event) {
    scrolling = false;
});


$(".down").bind("click", function(event) {
    event.preventDefault();
    $(".bid-list").animate({
        scrollTop: "+=" + step + "px"
    });
}).bind("mouseover", function(event) {
    scrolling = true;
    scrollContent("down");
}).bind("mouseout", function(event) {
    scrolling = false;
});

	$('.js-data-example-ajax').select2({
    minimumInputLength: 2,
		placeholder:'Search users',
    ajax: {
        url: '/user/find',
        dataType: 'json',
        type: "GET",
        quietMillis: 50,
        data: function (term) {
            return 'where='+JSON.stringify({username:{contains:term.term}});
        },
				processResults: function (data, params) {
						console.log(data);
						return {
							results: data,
							pagination: {
								more: false
							}
						};
					},
					cache: true
				},
				escapeMarkup(markup){
					return markup;
				},
				templateResult: function(data){
					return 	'<a href="/UserProfile/' + data.username + '"/>' +
					'<div class="panel panel-info">' +
					'<div class="panel-body">' +
					 data.username +
					'</div></div></a>'
				},

				width: 'resolve',
				dropdownAutoWidth : true
    });
})
	

</script>
