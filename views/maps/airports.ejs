<script src="/js/dependencies/g-maps.js"></script>
<script>
    $(document).ready(function(){
       var sf_map = _seat_filla_map({
                    enableHighAccuracy: true,
                    timeout: 6000,
                    maximumAge: 0,
                    desiredAccuracy: 30,
                    fallbackToIP: true,
                    addressLookup: true,
                    timezone: true,
                    useCache:true,
                    preFetchLocation:true,
                    maxOpenMarkers:1
        });

    });
</script>
<body class="fullbleed layout vertical">
      <%- partial ('../partials/nav.ejs') %>
