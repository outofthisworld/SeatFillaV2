nav.left-nav {
 
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
nav.left-nav ul {
	background-color: #14B9FF;
  width: 250px;
  margin-top: 50px;
	display:none;
	height:100%;
}
nav.left-nav ul:hover li {
  color: rgba(255, 255, 255, 0.5);

}
nav.left-nav ul li {
  transition-duration: 0.25s;
  display: block;
  color: white;
  letter-spacing: 0.02em;
  text-align: center;
	padding:20px;
}
nav.left-nav ul li:hover {
  cursor: pointer;
  color: white;
}
.openNav {
  transition-duration: 0.5s;
  transition-timing-function: swing;
  min-width: 50px;
  min-height: 50px;
	width:40px;
  background-color: #14B9FF;
  position: fixed;
	top:5px;
  left: 52px;
  z-index: 9;
	border-radius:30px;
}
.openNav:hover {
  cursor: pointer;
}
.openNav .icon {
  transition-duration: 0.2s;
  position: absolute;
  width: 30px;
  height: 2px;
  background-color: white;
  top: 24px;
  left: 10px;
}
.openNav .icon:before,
.openNav .icon:after {
  transition-duration: 0.5s;
  background-color: white;
  position: absolute;
  content: "";
  width: 30px;
  height: 2px;
  left: 0px;
}
.openNav .icon:before {
  top: -7px;
}
.openNav .icon:after {
  top: 7px;
}
.openNav.open {
  
}
.openNav.open .icon {
  background-color: transparent;
}
.openNav.open .icon:before,
.openNav.open .icon:after {
  top: 0;
}
.openNav.open .icon:before {
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
}
.openNav.open .icon:after {
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}
.navOpen {
  overflow: hidden;
}
.wrapper {
  transition-duration: 0.5s;
  transition-timing-function: swing;
  width: 100%;
  height: 100%;
  position: relative;

}
.wrapper.open {
  transform: translateX(250px);
  -webkit-transform: translateX(250px);
  box-shadow: 0 0 50px rgba(0, 0, 0, 0.2);
}

